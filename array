#include <bits/stdc++.h>

using namespace std;

int main()
{
    
    
    //stl array declare karte time bo only constant value leta hai
    //ye static hota hai
    
    array<int,5>a;
    
    for(int i=0;i<5;i++)
    {
        cin>>a[i];
    }
    
    //.size() se size niklega
    int size = a.size();
    
    cout<<"size is: "<<size<<endl;
    
    for(int i=0;i<5;i++)
    {
        cout<<a[i]<<" ";
    }

    //at() operation
    cout<<endl;
    cout<<"element at 2nd index is: ";
    cout<<a.at(2);
    
    //empty() operation
    // agar empty nhi hoga to 0 return karega else 1 karega
    cout<<endl;
    cout<<"empty or not: "<<a.empty();
    
    //front operation stl array ka pehla element return kar dega
    cout<<endl;
    cout<<"first element is: "<<a.front();
    
    //back operation stl array ka last element return karta hai
    cout<<endl;
    cout<<"last element is: "<<a.back();
    
    return 0;
}
